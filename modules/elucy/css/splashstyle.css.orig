/* border of Site specific content and containers for content */

	div.primaryouter {	
		border-left: 2px solid black;
		border-right:2px solid black;
	}


	div.primary {
		padding-bottom:15px;
		background-color: #F7EFD1;
		border-left: 1px solid black;
		border-right: 1px solid black;
		border-bottom: 1px solid black;
	}


	div.primary a{
		text-decoration:none;
		font-weight:bold;
		font-style: trebuchet ms;
		color:black;
	}

/* end of containers */


/* last conatiner for pictures and info */

	div.mainsite {
		height:auto;
		padding-bottom:15px;
	}


	div.mainsite h2{
		text-align: center;
		font-size: 1.7em;
		font-weight:bold;
		text-decoration: none;
	}

/* end of mainsite conatiner */



/* featured site picture set to 400px */

	div.feature {
		float:left;
		width:45%;
		background-color: transparent;
		margin-left: 20px;
		padding-top:5px;
		padding-right:15px;
		padding-bottom:20px;
	}


	div.feature img{
		position:relative;
		width:400px;
		border: 15px solid #ffb400;
		padding:2px;
		background-color: white;
	}


/* end of featured picture */



/* accompanying information for specific attribute of specified site */

	div.info {
		margin-top:5px;
		margin-left: 20px;
		border:1px solid black;
		float:left;
		width:400px;
		padding:5px;
		background-color:white;
		text-decoration:none;
	}


	div.info p{
		font-family: trebuchet ms;
		font-size: 1.1em;
		text-align: center;
		background-color: transparent;
	}


	div.info h3 {
		font-weight: bold;
		padding-top:5px;
		font-size: 1.1em;
		width:auto;
		float:left;
		padding-left:12%;
		font-family:trebuchet ms;
	}


	div.info h4 {
		font-weight: bold;
		padding-top:5px;
		font-size: 1.1em;
		width:auto;
		float:left;
		padding-left:10px;
		font-family:trebuchet ms;
	}


	div.info a {
		text-decoration:none;
		float:left;
		width:auto;
		
	}


	div.info img{
		float:left;
		padding:5px;
	}


/* end of info */



	div.foot {
		float:left;
		background-color: white;
		height:50px;
		border-bottom: 3px solid black;
		border-top: 2px solid black;
		border-left:3px solid black;
	}


	div.foot ul{
		list-style: none;
	}


	div.foot li {
		border-right: 2px solid black;
		background-color: #A69C81;
		float:left;
		width: 189.2px;
		height: 40px;
		text-align: center;
		padding-top:10px;
	}


	div.foot ul li.lastone {
		border-right: 3px solid black;
		background-color:  #A69C81;
		float:left;
		width: 189.2px;
		height: 40px;
		text-align: center;
		padding-top:10px;

	}


	div.foot li a{
		position:relative;
		top:6px;

	}


	div.foot li:hover a{
		color: #F7EED1;
	
	}


	div.foot a {
		 text-decoration: none;
		 font-weight: bold;
		color: black;
	}


	div.foot a:first-letter{
		font-size:1.3em;
	}