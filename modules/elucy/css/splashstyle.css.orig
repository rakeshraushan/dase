html,body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,input,textarea,p,blockquote,th,td { margin:0; padding:0; }

img { border:0; }
ol,ul { list-style:none; }
h1,h2,h3,h4,h5,h6 { font-size:100%; font-weight:normal; }


body{
font-family: arial,sans-serif;
font-size: small;
color: black;
background-color: #766e57;
}

div.header {
	   padding-left:10px;
background-image: url(images/header_grade.jpg);
background-repeat: repeat-y;
border: 2px solid black;
}

div.header img {
		  
position: relative;
top:-15px;
left:-24px;
	 margin-bottom:-18px;
width:260px;
height:59px;
	 

}

div.header_controls {
float: right;
	   margin-right: 20px;
	   margin-top: 12px;
}

div.header_controls form {
float: left;
	   margin-left: 10px;
}

div.container{
	width:960px;
	margin:0 auto;
}

div.branding {

	   text-align:right;
color: #d9cfb4;
}

div.branding a {
color: #d9cfb4;
	   text-decoration: none;
}

div.spacer {
clear: both;
}

div.timeline {
border-left: 2px solid black;
border-right: 2px solid black;
background-color: #F7EED1;
}







/* Site Selector begins above time line */

	div.sites {
		position:relative;
		top:0px;
		left:0px;
		border-top: 1px solid black;
		border-left:1px solid black;
		border-right:1px solid black;
		background-color: #C7CEE0;
		height:60px;
		width:954px;	
	}


	div.sites ul {	
		list-style:none;					
	}


	div.sites ul li a{
		font-size: .8em;
		font-family: trebuchet ms, arial;
		font-weight: bold;

	}
	

	div.sites ul li.middleawash{
		position:absolute;
		top:25%;
		left:74.41%;
		width:18.3%;	
		background-color: transparent;
		text-align:center;
		border-bottom: 8px solid black;
	
	}


	div.sites ul li.middleawash:hover {	
		border-bottom: 8px solid #ffb400;					
	}


	div.sites ul li.hadar{
		position:absolute;
		top:25%;
		left:47.74%;		
		width:10%;					
		background-color: transparent;
		text-align:center;
		border-bottom: 8px solid black;
	}


	div.sites ul li.hadar:hover {	
		border-bottom: 8px solid #ffb400;
					
	}


	div.sites ul li.olduvai{
		position:absolute;
		top:25%;
		left:17.74%;	
		width:16.2%;						
		background-color:transparent;
		text-align:center;
		border-bottom: 8px solid black;
	}


	div.sites ul li.olduvai:hover {	
		border-bottom: 8px solid #ffb400;					
	}


	div.sites ul li.zhoukoudian{
		position:absolute;
		top:40%;
		left:7.74%;	
		width:6%;						
		background-color: transparent;
		text-align:center;
		border-bottom: 8px solid black;
	}


	div.sites ul li.zhoukoudian:hover {	
		border-bottom: 8px solid #ffb400;					
	}


	div.sites ul li.atapuerca{
		position:absolute;
		top:0%;
		left:6.07%;	
		width:3.3%;						
		background-color: transparent;
		text-align:center;
		border-bottom: 8px solid black;
	}


	div.sites ul li.atapuerca:hover {	
		border-bottom: 8px solid #ffb400;					
	}


/* Site Selector ends */






/* Time Line (yellow background with scale and indentions) */



	div.scale{
		position:relative;
		top:58px;
		left:10px;
		height:4px;
		background-color:black;
		width:934px;
	}


	div.years {
		position:absolute;
		top:10px;
		left:0px;
		background-color: transparent;		
		width:934px;
	
	}


	div.years ul a{
	  	 font-size: 1.3em;
	}


	div.years ul li.first{
		position:relative;
		top:-16px;
		left:0px;		
		width:16.2%;
		float:left; 
		height: 16px;	
		text-align:left; 
		background-color: transparent;
		border-left: 4px solid black;
	}


	div.years ul li.first a{
		position:relative;
		top:14px;
		left:0px;
	}


	div.years ul li.middle{	
		position:relative;
		top:-16px;
		left:0px;
		width: 16.2%;
		float:left;
		height: 16px;
		text-align:left;
		border-left: 4px solid black;
		background-color:transparent;
	}


	div.years ul li.middle a{
		position:relative;
		top:14px;
		left:-15px;
	}


	div.years ul li.last{
		position:absolute;
		top:-16px;
		left:930px;
		float:left;
		height:16px;		
		text-align:left;	
		background-color: transparent;
		border-left: 4px solid black;
 
	}


	div.years ul li.last a{
		position:relative;
		top:14px;
		left:-17px;
	}


	div.bar {
		height:22px;
		background-color:#ffb400;
		border-left:1px solid black;
		border-right:1px solid black;
	}


/* end of Time line */


/* conatiner for attributes (maybe unnecessary) */

	div.mainsiteattributes {
		text-align: center;
		text-decoration: none;	 
	}


/* end of container */


