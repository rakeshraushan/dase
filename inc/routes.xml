<routes>

  <route name="list_collections" auth="user" params="0">
	<match>collections</match>
	<match></match>
  </route>

  <route name="browse_collection" auth="user" params="0">
	<match params="1">collection</match>
	<match params="2">collection</match>
	<match>(.*_collection)</match>
	<match params="3">^(.*_archive)</match>
	<match auth="">^(.*_catalog)</match>
  </route>

  <route name="collection_xml_dump" auth="token" params="1">
	<match>xml</match>
  </route>

  <route name="item_xml_dump" auth="token" params="1">
	<match>xml/item</match>
  </route>

  <route name="ajax_attributes" auth="none" params="0">
	<match>ajax/attributes</match>
  </route>

  <route name="ajax_admin_attributes" auth="none" params="0">
	<match>ajax/admin_attributes</match>
  </route>

  <route name="ajax_attribute_tallies" auth="none" params="0">
	<match>ajax/attribute_tallies</match>
  </route>

  <route name="login_form" params="0"/>
  <route name="login" params="0"/>
  <route name="logoff" auth="user" params="0"/>
  <route name="view_log" auth="admin" params="1">
	<match params="1">log</match>
  </route>
  <route name="build_index" auth="admin" params="1"/>

</routes>
