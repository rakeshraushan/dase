DirectoryIndex index.php index.html
AddType application/x-httpd-php .php
Options FollowSymLinks
FileETag none

RewriteEngine On
#RewriteBase  /dase  
RewriteBase  /dasetest  

# required for load-balancer keepalive (ladd)
#RewriteCond %{REMOTE_ADDR} 10.0.0.250
#RewriteRule .*             - [L]

#force https
#RewriteCond %{SERVER_PORT} 80 
#RewriteRule ^(.*)$ https://dase.lib.utexas.edu/$1 [R=permanent,L]

php_flag magic_quotes_gpc off

# media files go straight to media.php
RewriteRule ^media/([0-9a-z_]*)/([a-z]*)/([^/]*)$ media.php?collection=$1&size=$2&filename=$3 [PT]
RewriteRule ^([0-9a-z_]*)/media/([a-z]*)/([^/]*)$ media.php?collection=$1&size=$2&filename=$3 [PT]

RewriteCond %{REQUEST_FILENAME} !-f 
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . index.php [PT] 

